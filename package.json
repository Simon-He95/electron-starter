{
  "name": "electron-starter",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.18.3",
  "description": "An Electron application with Vue and TypeScript",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "main": "./out/main/index.cjs",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "dev:debug": "DEBUG=true ELECTRON_DEBUG=true pnpm dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:macarm": "npm run build && electron-builder --mac --arm64",
    "build:linux": "npm run build && electron-builder --linux",
    "test:schemas": "node ./scripts/test-schema.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "npm run typecheck && npm run test && npm run test:schemas"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^3.0.0",
    "@simon_he/v-axios": "^0.0.10",
    "dexie": "^4.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^24.7.2",
    "@vee-validate/zod": "^4.15.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vueuse/core": "^13.9.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron": "^37.7.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "embla-carousel-vue": "^8.6.0",
    "eslint": "9.34.0",
    "lazy-js-utils": "^0.1.49",
    "lucide-vue-next": "^0.542.0",
    "pinia": "^3.0.3",
    "prettier": "^3.6.2",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.5.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-scrollbar-hide": "^4.0.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "uuid": "^11.1.0",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vite": "^7.1.10",
    "vite-plugin-pages": "^0.33.1",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^1.6.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.2",
    "vue-sonner": "^2.0.9",
    "vue-tsc": "^3.0.9",
    "zod": "^4.1.12"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "electron-winstaller",
      "esbuild"
    ]
  },
  "overrides": {
    "rollup": "4.44.1"
  },
  "resolutions": {
    "rollup": "4.44.1"
  }
}
